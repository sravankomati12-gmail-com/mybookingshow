<!DOCTYPE html>
<html>
    <head>
        <style>
        table {
            border-collapse: collapse;
        }
        table, th, td {
            border: 1px solid black;
        }
        </style>
    </head>
    <body>
        <div style="width: 85%; margin: 0 7.5%;">
            <div style="text-align: center; padding-left: 50px;">            
                <h3>Movies Report</h5>
                <table >
                    <tr>
                        <th>Movie name</th>
                        <th>decription</th>
                        <th>language</th>
                        <th>releaseDate</th>
                        <th>rating</th>
                        <th>director</th>
                        <th>producers</th>
                        <th>casting</th>
                    </tr>
                    <tbody>
                <% if (data.length > 0) { %>
                    <% data.forEach(item => {%>
                       <tr>
                        <td><%= item.name %></td><td><%= item.decription %></td>
                        <td><%= item.language %></td><td><%= item.releaseDate %></td>
                        <td><%= item.rating %></td><td><%= item.director %></td>
                        <td><%= item.producers %></td><td><%= item.casting %></td>
                    </tr>
                    <%  });%>
                <% }%></tbody>
                </table>
             </div>
        </div>
    </body>
</html>